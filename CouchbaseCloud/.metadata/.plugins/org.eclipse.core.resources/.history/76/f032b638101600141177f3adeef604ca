<div class="helpTray">
  <a class="openHelp" onclick="HelpTray.openHelpTray();"><div class="openHelpButton"></div></a>
  <a class="closeHelp" onclick="HelpTray.closeHelpTray();"><div class="closeHelpButton"></div></a>
  <div class="defaultHelp">
    <h1></h1>
    
  </div>
