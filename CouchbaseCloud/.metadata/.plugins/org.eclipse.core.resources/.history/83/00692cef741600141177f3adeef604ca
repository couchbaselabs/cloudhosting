<script type="text/javascript">
    $(document).ready(function() {
        // populate capabilities
        for(var p in PACKAGE_INFO) {
            var capabilities = PACKAGE_INFO[p].capabilities;
            var $planDiv = $("#"+p);
            for(var c in capabilities) {
                if(capabilities[c]) {
                    var $capability = $planDiv.find(".capability."+c);
                    $capability.addClass("hasCapability");
                    $capability.html("&#149;");
                }
            }
        }
        var $allCapabilities = $(".capability.all");
        $allCapabilities.html("&#149;");
        $allCapabilities.addClass("hasCapability");

        // bind help
        bindSpecificHelp($(".capability.hasCapability.emergencySupport"), "Emergency Support");
        bindSpecificHelp($(".capability.hasCapability.unlimitedDbs"), "Create Unlimited Databases");
        bindSpecificHelp($(".capability.hasCapability.dedicatedVM"), "Dedicated Virtual Machines");
        bindSpecificHelp($(".capability.hasCapability.dedicatedMongod"), "Dedicated mongod Process");
        bindSpecificHelp($(".capability.hasCapability.logFileAccess"), "Log File Access");
        bindSpecificHelp($(".capability.hasCapability.scheduledBackups"), "Scheduled Backups");
        bindSpecificHelp($(".capability.hasCapability.continuousMonitoring"), "Continuous Monitoring");
        bindSpecificHelp($(".capability.hasCapability.mmsAgent"), "MMS Agent");
    });
</script>
