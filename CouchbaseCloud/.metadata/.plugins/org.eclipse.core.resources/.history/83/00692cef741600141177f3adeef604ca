    <input type="radio" name="cloneType" value="uri"/>
    Provide URI for a cloud-accessible MongoDB
  </label>
  <div style="margin-left: 20px;">
    <input type="text" name="cloneFromURI" id="cloneFromURI" style="width: 530px"/>
    <div id="allowCloneUserCreationDiv" style="display:none; margin-top:8px">
      <input type="checkbox" name="allowCloneUserCreation" id="allowCloneUserCreation" value="true">
      Allow MongoLab to create a temporary user in the source database
      </input>
    </div>
  </div>
</div>

<script type="text/javascript">
  $(document).ready(function() {
    $('#cloneFromDB').bind('focus', function() {
      selectCloneType("db");
    });
    var $cloneFromURI = $("#cloneFromURI");
    if($cloneFromURI.length) {
      $cloneFromURI.rules("add", {
        required: function (element) {
          return isCloningFromURI();
        },
        mongouri: {
          depends: function (element) {
            return isCloningFromURI();
          }
        }
      });
      $cloneFromURI.bind('focus', function() {
        selectCloneType("uri");
      });
      $cloneFromURI.keyup(updateAllowUserCreationCheckbox);
      $cloneFromURI.change(updateAllowUserCreationCheckbox);
      $("#allowCloneUserCreation").rules("add", {
        required: function (element) {
          return isCloningFromURI() && isCloneUserCreationRequired();
        }
      });
    }
  });

  function isCloningFromURI() {
    return getCloneType() === "uri";
  }

  function isCloningFromClusterDB() {
    return getCloneType() === "clusterDb";
  }

  function getCloneType() {
    return $('input[name=cloneType]:checked', '#createResourceForm').val();
  }

  function selectDefaultCloneType() {
    $("input:radio[name=cloneType]").each(function() {
      if($(this).css("display") != "none") {
        selectCloneType($(this).val());
        return false;
      }
    });
  }

  function selectCloneType(cloneType) {
    $('input:radio[name=cloneType]').removeAttr("checked");
    $('input:radio[name=cloneType][value=' + cloneType + ']').attr('checked', true);
    updateAllowUserCreationCheckbox();
  }

  function hideAllowUserCreationCheckbox() {
    $("#allowCloneUserCreationDiv").hide();
    $("#allowCloneUserCreation").attr("checked", false);
  }

  function showAllowUserCreationCheckbox() {
    $("#allowCloneUserCreationDiv").show();
  }

  function isCloneUserCreationRequired() {
    var uri_parts = parseMongoUri($("#cloneFromURI").attr("value"));
    return !(!uri_parts || (uri_parts["username"] && uri_parts["password"]));
  }

  function updateAllowUserCreationCheckbox() {
    if(isCloningFromURI() && isCloneUserCreationRequired()) {
      showAllowUserCreationCheckbox();
    } else {
      hideAllowUserCreationCheckbox();
    }
  }

  function initDatabaseSelector() {
    var $cloneDbSection = $("#cloneHostedDbSection");
    if(!$($cloneDbSection).hasClass("loaded")) {
      // populate hosted dbs
      populateCloneHostedDbs();
      $($cloneDbSection.addClass("loaded"));
    }
    $('input:radio[name=cloneType]').removeAttr("disabled");
    selectDefaultCloneType();
  }

  function disableDatabaseSelector() {
    $('input:radio[name=cloneType]').removeAttr("checked");
    $('input:radio[name=cloneType]').attr("disabled", "disabled");
  }

  function populateCloneHostedDbs() {
    getJson(getRootApiUrl()+"/databases?apiKey=HyDTfe2yMeS_dKYK8zSCc4p7qjwdXHUg&multiTenantOnly",
        function(data) {
          if(data && data.length > 0) {
            var $dbSelect = $("#cloneFromDB");
            $dbSelect.html("");
            for(var i = 0; i < data.length; i++) {
              $dbSelect.append('<option value="'+data[i]+'">'+data[i]+'</option>');
            }
            selectDefaultCloneType();
          } else {
            $("#cloneHostedDbSection").hide();
            selectDefaultCloneType();
          }
        },
        function (data) {
          $("#cloneHostedDbSection").hide();
          selectDefaultCloneType();
        });
  }

</script>

    </div>
</div>


