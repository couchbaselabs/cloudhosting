// build PROVIDER_INFO

PROVIDER_INFO.push(
        {"id": "AWS",
            "preferred" : "true",
            "beta" : "",
            "displayName" : "",
            "confirmationMessage" : ""});

PROVIDER_INFO.push(
        {"id": "GCP",
            "preferred" : "",
            "beta" : "",
            "displayName" : "",
            "confirmationMessage" : ""});

PROVIDER_INFO.push(
        {"id": "JYC",
            "preferred" : "",
            "beta" : "",
            "displayName" : "",
            "confirmationMessage" : ""});

PROVIDER_INFO.push(
        {"id": "RSC",
            "preferred" : "",
            "beta" : "",
            "displayName" : "",
            "confirmationMessage" : ""});

PROVIDER_INFO.push(
        {"id": "AZR",
            "preferred" : "",
            "beta" : "",
            "displayName" : "",
            "confirmationMessage" : ""});


function getSortedProviderIds() {
    var ids = [];
    for(var i = 0; i < PROVIDER_INFO.length; i++) {
        var id = PROVIDER_INFO[i].id;
        if(id && id in PROVIDER_CLOUD_PLANS) {
            ids.push(id);
        }
    }
    return ids;
}

function getProviderInfo(providerId) {
    for(var i = 0; i < PROVIDER_INFO.length; i++) {
        var info = PROVIDER_INFO[i];
        if(info.id == providerId) {
            return info;
        }
    }
    return null;
}

function makeProviderConfirmationHandler(providerId) {
    return function() {
        selectProvider(providerId, true);
    }
}

function populateProviders() {
    var $providerList = $("#providerList");
    var ids = getSortedProviderIds();
    for(var i in ids) {
        var p = ids[i];
        $providerList.append('<div class="provider" id="'+p+'"><div class="icon"/><div class="providerLabel"></div></div>');
        // apply additional configs
        var info = getProviderInfo(p);
        if(info) {
            var $provider = $("#"+p);
            // preferred?
            if(info.preferred) {
                $provider.addClass("preferred");
            }
            // confirmation message?
            if(info.confirmationMessage) {
                $provider.addClass("confirmationRequired");
                // create a confirmation dialog
                var dialogId = p+'ConfirmationDialog';
                $("body").append('<div id="'+dialogId+'" title="Accept preview subscription?">'+info.confirmationMessage+'</div>');
                initYesNoDialog("#"+dialogId, 400, "auto", null, makeProviderConfirmationHandler(p));
            }
            // display name?
            if(info.displayName) {
                $provider.find(".providerLabel").html(info.displayName);
            }
        }
    }
    // add event handlers
    $providerList.find(".provider").click(function(event) {
        event.stopPropagation();
        if($(this).hasClass("selected")) {
          // this only has an effect in dropdown mode
          var $providerList = $("#providerList");
          if($providerList.hasClass("reveal")) {
            $providerList.removeClass("reveal");
          } else {
            $providerList.addClass("reveal");
          }
        } else {
          selectProvider($(this).attr('id'));
        }
    });
    // close dropdowns when clicking away
    $('html').click(function() {
      $("#providerList.reveal").removeClass("reveal");
      $("#cloudSelect.reveal").removeClass("reveal");
    });
    selectDefaultProvider();
    selectDefaultUsage();
}

function selectDefaultProvider() {
    var defaultId = "";
    if(!defaultId) {
      var $default = $(".provider.preferred");
      if(!$default.length) {
        $default = $('.provider:first');
      }
      defaultId = $default.attr('id');
    }
    selectProvider(defaultId);
}

function selectProvider(providerId, seriously) {
    var $providerDiv = $("#"+providerId);
    if(!$providerDiv.hasClass("selected")) {
        if(!seriously && $providerDiv.hasClass("confirmationRequired")) {
            openDialog('#'+providerId+'ConfirmationDialog', null);
        } else {
            // highlight provider puck
            $('.provider').removeClass('selected');
            $providerDiv.addClass('selected');

            // only relevant for dropdown mode
            $("#providerList").removeClass("reveal");

            // populate clouds
            populateClouds(providerId);
        }
    }
}

function getSelectedProvider() {
    var $provider = $(".provider.selected");
    if($provider.length) {
        return $provider.attr("id");
    }
    return null;
}

function compareCloudInfo(a, b) {
    var da = a.description.toLowerCase();
    var db = b.description.toLowerCase();

    if(da == db) {
        return 0;
    } else if(da > db) {
        return 1;
    } else {
        return -1;
    }
}

function populateClouds(providerId) {
    var $cloudSelect = $("#cloudSelect");
    // remove existing content
    $cloudSelect.html("");
    // sort sub clouds
    var subClouds = PROVIDER_CLOUD_PLANS[providerId];
    var clouds = []
    for(var c in subClouds) {
        clouds.push(CLOUD_INFO[c])
    }
    clouds.sort(compareCloudInfo);
    // add sub clouds
    for(var i in clouds) {
        var cloudInfo = clouds[i];
        var cloudId = cloudInfo.id
        var preferredClass = "";
        if(cloudInfo.preferred) {
            preferredClass = "preferred";
        }
        $cloudSelect.append('<div id="'+cloudId+'" class="'+preferredClass+'">'+cloudInfo.description+' ('+cloudInfo.providerId+')</div>');
    }
    // add click handler
    $cloudSelect.find("div").click(function(event) {
      event.stopPropagation();
      if($(this).hasClass("selected")) {
        var $cloudSelect = $("#cloudSelect");
        if($cloudSelect.hasClass("reveal")) {
          $cloudSelect.removeClass("reveal");
        } else {
          $cloudSelect.addClass("reveal");
        }
      } else {
        selectCloud($(this).attr('id'));
      }
    });
    // pre-select the preferred cloud
    selectPreferredCloud();
}

function selectPreferredCloud() {
    var $preferred = $("#cloudSelect").find(".preferred");
    if($preferred.length) {
        selectCloud($preferred.attr("id"));
    } else {
        // just pick the first one
        selectCloud($("#cloudSelect").find("div:first").attr("id"));
    }
}

function selectCloud(cloudId) {
    if(CLOUD_INFO[cloudId]) {
        selectProvider(CLOUD_INFO[cloudId].providerCloudId);
        $("#cloudValue").val(cloudId);
        $("#cloudSelect div.selected").removeClass("selected");
        $("#"+cloudId).addClass("selected");
        $("#cloudSelect").removeClass("reveal");
        $("#confirmLocation").html(CLOUD_INFO[cloudId].description+' ('+CLOUD_INFO[cloudId].providerId+')');
        refreshPlans();
    }
}

function getSelectedCloud() {
    return $("#cloudSelect div.selected").attr("id");
}

function refreshPlans() {
    var usageClass = "usage"+getSelectedUsage();
    var cloudClass = getSelectedCloud()+"Plan";
    // hide/show tabs
    for(var i in [0,1,2]) {
        if($(".plan."+cloudClass+".usage"+i).length == 0) {
            $("#usage"+i).hide();
        } else {
            $("#usage"+i).show();
        }
    }
    $(".plan").hide();
    var $plansToShow = $(".plan."+usageClass+"."+cloudClass);
    if($plansToShow.length) {
        $("#noPlans").hide();
        $plansToShow.show();
        $(".plans").show();
        if($(".plan.selected:visible").length == 0) {
            selectDefaultPlan();
        } else {
            // re-select plan to refresh prices
            selectPlan(getSelectedPlan());
        }
        refreshPlanPrices();
    } else {
        selectDefaultUsage();
    }
}

function refreshPlanPrices() {
    $(".plan:visible").each(function() {
        var info = PACKAGE_INFO[$(this).attr("id")];
        var price = info.base;
        var numNodes = getSelectedUsage();
        if(numNodes > 1) {
            price = price * numNodes * info.multiNodeMultiple;
        }
        $(this).find(".planPrice").html(Math.round(price));
    });
    refreshTotalPrice();
}

function selectPlan(planId) {
    if(planId) {
        var $planDiv = $("#"+planId+":visible");
        if($planDiv.length) {
            if(!$planDiv.hasClass("selected")) {
                // unselect all plans
                $('.plan').removeClass('selected');
                // select plan
                $planDiv.addClass('selected');

                // uncheck plans
                $("input[name=plan]").attr('checked', false);
                // check selected plan
                $planDiv.find("input").attr('checked', true);
            }

            // show credit card warning if for a pay plan is selected
            refreshCardWarning();
            refreshMongoVersionOptions();
            refreshExperimentalOption();
            refreshStorageOptions();
            refreshTotalPrice();
            refreshInfoText();
            $("#confirmPlan").html(PACKAGE_INFO[planId].name+" ("+$planDiv.find(".ram").html().trim()+" RAM)");
        } else {
            selectDefaultPlan();
        }
    }
}

function getSelectedPlanDiv() {
    var $div = $(".plan.selected:visible");
    if($div.length) {
        return $div;
    } else {
        return null;
    }
}

function getSelectedPlan() {
    var $div = getSelectedPlanDiv();
    if($div) {
        return $div.attr("id");
    }
    return null;
}

function refreshCardWarning() {
    var $div = getSelectedPlanDiv();
    if($div && $div.hasClass("forPayPlan")) {
        showCardWarning();
    } else {
        hideCardWarning();
    }
}

function showCardWarning() {
    $("#cardWarning").show();
}

function hideCardWarning() {
    $("#cardWarning").hide();
}

function selectDefaultPlan() {
    var $plan = $(".plan.preferred:visible:first");
    if($plan.length) {
        selectPlan($plan.attr("id"));
    } else {
        selectFirstPlan();
    }
}

function selectFirstPlan() {
    var $first = $(".plan:visible:first");
    if($first.length) {
        selectPlan($first.attr("id"));
    }
}

function selectDbName(name) {
    $("#dbName").val(name);
    refreshPermalink();
}

function selectMongoVersion(ver) {
    $("#mongoVersionSelect").val(ver);
    $("#confirmVersion").html("MongoDB " + ver);
    refreshPermalink();
}

function selectDefaultUsage() {
    // if the default tab is visible
    var $tab = $("#tabs .default:visible a");
    if($tab.length) {
        $tab.click();
    } else {
        // click the first visible tab
        $("#tabs").find("li:visible:first a").click();
    }
}

function selectUsage(numNodes) {
    // select tab
    var tabIndex = $("#tabs").find("#usage"+numNodes).index('li');
    $("#tabs").tabs({selected:tabIndex});
    // select radio
    $("#usage"+numNodes+"radio").attr("checked", true);
    var displayNum = numNodes;
    if(displayNum == 0) {
      displayNum = 1;
    }
    $("#confirmNumNodes").html(displayNum + " electable nodes");
    // show description
    $("#tabDescription div").hide();
    $("#tabDescription .usage"+numNodes).show();
    refreshPlans();
}

function getSelectedUsage() {
    return $("input[name=numNodes]:checked").val();
}

function refreshMongoVersionOptions() {

    $("#experimentalPlanDiv").show();
    var $versionList = $("#mongoVersionSelect");
    $versionList.find('option').remove();
    var plan = getSelectedPlan();
    var planInfo = null;
    if(plan in PACKAGE_INFO) {
        planInfo = PACKAGE_INFO[plan];
    }
    if(planInfo && "mongoVersion" in planInfo.options) {
        var versions = planInfo.options.mongoVersion.values;
        for(var i in versions) {
            if (plan == "ss") {
                $versionList.append('<option value="'+versions[i].value+'">'+versions[i].value.substring(0,4)+'x</option>');
  }
            else {
                $versionList.append('<option value="'+versions[i].value+'">'+versions[i].value+'</option>');
            }
        }
    }

    // select the default option if specified
    if(planInfo && "mongoVersion" in planInfo.options) {
        selectMongoVersion(planInfo.options.mongoVersion.default.value);
    } else {
        selectMongoVersion($versionList.find('option:first').val());
    }
}

function getExperimentalPlanInfo() {
    var selectedPlan = getSelectedPlan();
    var provider = getSelectedProvider();
    if(provider in PROVIDER_CLOUD_PLANS) {
        var cloudPlans = PROVIDER_CLOUD_PLANS[provider];
        var cloud = getSelectedCloud();
        if(cloud in cloudPlans) {
            var plans = cloudPlans[cloud];
            if(plans) {
                for(var i = 0; i < plans.length; i++) {
                    var plan = plans[i];
                    if(plan && plan.id == selectedPlan) {
                        if("experimentalPackage" in plan) {
                            return plan["experimentalPackage"];
                        } else {
                            return null;
                        }
                    }
                }
            }
        }
    }
    return null;
}

function refreshExperimentalOption() {
    // experimental option?
    if(getExperimentalPlanInfo()) {
        showExperimentalOption();
    } else {
        hideExperimentalOption();
    }
}

function showExperimentalOption() {
    $("#experimentalPlanDiv").show();
    $("#experimentalPlanCheckbox").attr("checked", false);
    var experimentalInfo = getExperimentalPlanInfo();
    // populate dialog text
    var confirmationInfo = experimentalInfo.confirmation;
    var $dialog = $("#confirmExperimentalPlanDialog");
    $dialog.attr("title", confirmationInfo.title);
    $dialog.dialog({"title":confirmationInfo.title});
    $dialog.html(confirmationInfo.message);
}

function hideExperimentalOption() {
    $("#experimentalPlanDiv").hide();
    $("#experimentalPlanCheckbox").attr("checked", false);
}

function selectExperimentalPlan(seriously) {
    if(seriously) {
        $("#experimentalPlanCheckbox").attr("checked", true);
        // add experimental mongoVersion option
        var $mongoVersionSelect = $("#mongoVersionSelect");
        var experimentalInfo = getExperimentalPlanInfo();
        var experimentalVersion = experimentalInfo.mongoVersion;
        var mangledVersion = "__"+experimentalVersion;
        $mongoVersionSelect.append('<option id="experimentalMongoVersionOption" value="'+mangledVersion+'" disabled>2.6.x (experimental plan)</option>');
        $mongoVersionSelect.val(mangledVersion);
        $("#experimentalPlanDiv").hide();
        $mongoVersionSelect.on("change.experimental", unselectExperimentalPlan);
    } else {
        openDialog("#confirmExperimentalPlanDialog", null);
    }
}

function unselectExperimentalPlan() {
    $("#experimentalPlanCheckbox").attr("checked", false);
    var $mongoVersionSelect = $("#mongoVersionSelect");
    $mongoVersionSelect.off("change.experimental");
    refreshMongoVersionOptions()
}

function refreshStorageOptions() {
    var $storageDiv = $("#storageOptionsSection").find(".volumes");
    $storageDiv.html("");
    var plan = getSelectedPlan();
    if(plan in PACKAGE_INFO && "volume" in PACKAGE_INFO[plan].options) {
        var opts = PACKAGE_INFO[plan].options.volume.values;
        if(opts && opts.length > 0) {
            for(var i in opts) {
                var opt = opts[i];
                var id = "storageOption"+i;
                $storageDiv.append('<label class="radioIndent storageOption" id="'+id+'"></label>');
                $storageDiv.find('#'+id).append('<input type="radio" name="volumeIndex" value="'+i+'" id="'+id+'-radio">');
                $storageDiv.find('#'+id).append('<span class="storageOptionLabel"><span class="storageOptionDescription">'+opt.description+'</span><span class="storageOptionPrice"></span></span>');
                if(opt.price) {
                    $storageDiv.find('#'+id+' .storageOptionPrice').append('$<span class="price">'+(opt.price * getSelectedUsage()).toFixed(0)+'</span>');
                } else {
                    $storageDiv.find('#'+id+' .storageOptionPrice').append("included");
                }
            }
            // remove storage price if there's only one option (and it's included)
            if(opts.length == 1) {
                if($storageDiv.find(".storageOptionPrice .price").length == 0) {
                    $storageDiv.find(".storageOptionPrice").html("");
                }
            }
            $storageDiv.find('label').click(function() {
                selectStorageOption($(this).find('input').val());
            });
            selectDefaultStorageOption();
            return;
        }
    }
    // no volume option, so just show the base storage choice
    var id = "storageOption0";
    var description = PACKAGE_INFO[plan].baseSizeInGB+" GB";
    $storageDiv.append('<label class="radioIndent storageOption selected" id="'+id+'"></label>');
    $storageDiv.find('#'+id).append('<input type="radio" name="volumeIndex" value="'+i+'" id="'+id+'-radio" checked="true">');
    $storageDiv.find('#'+id).append('<span class="storageOptionLabel"><span class="storageOptionDescription">'+description+'</span><span class="storageOptionPrice"></span></span>');
    $("#volumeValue").val("");
    $("#confirmStorage").html(description);
}

function selectDefaultStorageOption() {
    if($("#storageOptionsSection:visible").length) {
        selectStorageOption(0);
    }
}

function selectStorageOption(index) {
    if($("#storageOption"+index+"-radio:visible").length) {
        var plan = getSelectedPlan();
        $(".storageOption.selected").removeClass("selected");
        if(plan && plan in PACKAGE_INFO && "volume" in PACKAGE_INFO[plan].options) {
            $("#storageOption"+index).addClass("selected");
            $("#storageOption"+index+"-radio").attr("checked", true);
            $("#volumeValue").val(JSON.stringify(PACKAGE_INFO[plan].options.volume.values[index].value));
            $("#confirmStorage").html(PACKAGE_INFO[plan].options.volume.values[index].description);
        } else {
            $("#volumeValue").val("");
            $("#confirmStorage").html(getSelectedPlanDiv().find(".storage").html());
        }
        refreshTotalPrice();
    } else {
        selectDefaultStorageOption();
    }
}

function getSelectedStorageOption() {
    var $radio = $(".storageOption input:checked");
    if($radio.length) {
        return $radio.val();
    }
    return null;
}

function getSelectedStorageDiv() {
    var index = getSelectedStorageOption();
    if(index == null) {
        return null;
    } else {
        return $("#storageOption"+index);
    }
}

function refreshTotalPrice() {
    var total = 0;
    var $plan = getSelectedPlanDiv();
    if($plan.hasClass("forPayPlan")) {
        total += parseFloat($plan.find(".planPrice").html());
    }
    var $storageDiv = getSelectedStorageDiv();
    if($storageDiv && $storageDiv.length) {
        var $priceDiv = $storageDiv.find(".price");
        if($priceDiv.length) {
            total += parseFloat($priceDiv.html());
        }
    }
    var $totalPrice = $("#totalPrice");
    $totalPrice.html(total.toFixed(0));
    //noinspection JSUnresolvedFunction
    $totalPrice.commas();
    $("#confirmTotal").html($("#totalPriceDiv").html());

    refreshPermalink();
}

function refreshPermalink() {
    // refresh permalink
    var url = document.URL.split("?")[0]+"?"+generateQueryParams();
    $("#permalink").attr("href", url);
}

function refreshInfoText() {
    // set database name input label
    $("#dbName").addClass("required");
    if(getCloneType()) {
        $("#databaseNameHeader").html("New destination database name:");
        $(".formHeader h1").html("Clone existing database");
        $(".formSubHeader").html('<p>Fill out this form to clone an existing database into a new MongoDB deployment.</p><p>Alternatively, you can <a href="javascript:hideCloneResource()">create a new subscription from scratch</a>.</p>');
    }else if(false){
        $("#databaseNameHeader").html("New destination database name:");
        $(".formHeader h1").html("Create new subscription from backup");
        $(".formSubHeader").html('<p>Fill out this form to restore a backup into a new MongoDB deployment.</p><p>Alternatively, you can <a href="/create">create a new subscription from scratch</a>.</p>');
    }
    else {
        var plan = getSelectedPlan();
        var label = "Database name:"
        if(plan && plan in PACKAGE_INFO && PACKAGE_INFO[plan].capabilities) {
            var capabilities = PACKAGE_INFO[plan].capabilities;
            if("hasAdminPrivileges" in capabilities && capabilities["hasAdminPrivileges"]) {
                label = "Initial database name <span class='tip'>optional</span>:";
                $("#dbName").removeClass("required");
            }
        }
        $("#databaseNameHeader").html(label);
        $(".formHeader h1").html("Create new subscription");
        $(".formSubHeader").html('<p>Fill out this form to create a brand-new MongoDB deployment in the cloud location of your choice.</p><p>Alternatively, you can <a href="javascript:showCloneResource()">clone an existing deployment</a>.</p>');
    }
}

function confirmOrder() {
    var plan = getSelectedPlan();
    if(plan) {
        if(getSelectedPlanDiv().hasClass("provisionerDisabled")) {
            openDialog('#confirmProvisionMessage', null);
        } else {
            submitForm(true);
        }
    }
    return false;
}

function submitForm(seriously) {
    var plan = getSelectedPlan();
    if(plan) {
        if(!seriously && !PACKAGE_INFO[plan].free) {
            openDialog('#orderConfirmationDialog', null);
        } else {
            openModalLoadingIndicator('#createResourceForm', 'Please wait...');
            // process experimental version
            if($("#experimentalPlanCheckbox").attr("checked")) {
                // set plan ID
                $("input[name='plan']").val(getExperimentalPlanInfo().id);
                // set mongo version to un-mangled value
                var experimentalVersion = getExperimentalPlanInfo().mongoVersion;
                $("#experimentalMongoVersionOption").attr("value", experimentalVersion);
                $("#mongoVersionSelect").val(experimentalVersion);
            }
            $('#createResourceForm').submit();
        }
    }
    return false;
}

function maybeParse(str) {
    if((str.indexOf("{") == 0 && str.indexOf("}") == str.length-1) || (str.indexOf("[") == 0 && str.indexOf("]") == str.length-1)) {
        return JSON.parse(str);
    } else {
        return str;
    }
}

function processQueryParams() {
    var cloneType = "";
    var clone = "";
    if(clone || cloneType) {
        showCloneResource();
    }

    var restore = "";

    if(restore) {
        showRestoreResource();
    }
    var cloud = "" || "";
    if(cloud) {
        selectCloud(cloud);
    }

    var numNodes = "" || "";
    if(numNodes) {
        numNodes = parseInt(numNodes);
        selectUsage(numNodes);
    }

    var plan = "" || "";
    if(plan) {
        selectPlan(plan);
    }

    var volumeIndex = "" || "";
    if(volumeIndex) {
        volumeIndex = parseInt(volumeIndex);
        if(volumeIndex >= 0) {
            selectStorageOption(volumeIndex);
        }
    }

    var dbName = "" || "";
    if(dbName) {
        selectDbName(dbName);
    }

    var mongoVersion = "" || "";
    if(mongoVersion) {
        selectMongoVersion(mongoVersion);
    }
}

function generateQueryParams() {
    var query = "";
    query += "c="+getSelectedCloud()+"&";
    query += "n="+getSelectedUsage()+"&";
    query += "p="+getSelectedPlan()+"&";
    query += "v="+getSelectedStorageOption()+"&";
    query += "d="+$("#dbName").val()+"&";
    query += "m="+$("#mongoVersionSelect").val();
    return query;
}

function toggleFeatures() {
    if($(".plan .capability:visible").length) {
        hideFeatures();
    } else {
        showFeatures();
    }
}

function showFeatures() {
    $(".plan .capabilitiesGroup").slideDown();
    $(".plan .capabilitiesGroup").promise().done(function() {
        $(".planLabel .capabilitiesGroup").css("opacity", 0);
        $(".planLabel .capabilitiesGroup").animate({width: 'toggle', opacity: 1});
    });
}

function hideFeatures() {
    $(".planLabel .capabilitiesGroup").animate({width: 'toggle', opacity: 0});
    $(".planLabel .capabilitiesGroup").promise().done(function() {
        $(".planLabel .capabilitiesGroup").hide();
        $(".plan .capabilitiesGroup").slideUp();
        $(".capabilitiesGroup").promise().done(function() {
            $(".capabilitiesGroup").hide();
        });
    });
}

function hideCloneResource() {
    if(History.getState().url.indexOf("/create") < 0) {
        History.pushState(null, null, "/create");
    }
    $(".cloneResource").hide();
    disableDatabaseSelector();
    refreshInfoText();
}

function showCloneResource() {
    if(History.getState().url.indexOf("/clone") < 0) {
        History.pushState(null, null, "/clone");
    }
    // reveal UI
    var $cloneSection = $(".cloneResource");
    $cloneSection.show();
    initDatabaseSelector();
    refreshInfoText();
}

function showRestoreResource() {
    if(History.getState().url.indexOf("/restore") < 0) {
        History.pushState(null, null, "/restore");
    }
    // reveal UI
    var restoreSection = $(".restoreResource");
    restoreSection.show();
}

// handle back button when switching between /create and /clone
History.Adapter.bind(window,'statechange', function() {
    if(History.getState().url.indexOf("/clone") > 0) {
        showCloneResource();
    } else {
        hideCloneResource();
    }
});

$(document).ready(function() {
    $("#createResourceForm").show();
    // setup usage tabs
    $("#tabs").tabs();
    $("#tabs ul").show();
    // init form
    validateForm("#createResourceForm");
    populateProviders();

    // init dialogs
    initYesNoDialog("#confirmProvisionMessage", 400, "auto", null, function() { submitForm(true); });
    initYesNoDialog("#orderConfirmationDialog", "auto", "auto", null, function() { confirmOrder(); });
    initYesNoDialog("#confirmExperimentalPlanDialog", 400, "auto", function() { unselectExperimentalPlan() }, function() { selectExperimentalPlan(true); });

    processQueryParams();
});
</script>

</body>
</html>
