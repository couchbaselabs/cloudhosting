<script type="text/javascript">
    // bind click handlers
    $(document).ready(function() {
        // click handler
        $(".plans .plan").click(function() {
            selectPlan($(this).attr("id"));
        });

        $("#mongoVersionSelect").change(function() {
            selectMongoVersion($(this).val());
        });

        // experimental plan checkbox
        $("#experimentalPlanCheckbox").change(function() {
            if($(this).attr("checked")) {
                selectExperimentalPlan();
            } else {
                unselectExperimentalPlan();
            }
        });
        // clicking on the text should be a toggle
        $("#experimentalPlanDiv span").click(function() {
            if($(this).attr("checked")) {
                unselectExperimentalPlan();
            } else {
                selectExperimentalPlan();
            }
        });

        // feature matrix toggle
        $(".plan .moreInfo").click(function() {toggleFeatures();});

        bindSpecificHelp($(".plan.hidden"), "HIDDEN PLAN");
        bindSpecificHelp($("#experimentalPlanDiv"), 'Experimental Databases');
        bindSpecificHelp($("#allowCloneUserCreationDiv"), 'Temporary source database user');
    });
</script>

